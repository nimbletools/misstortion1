ifndef config
  config=debug_x32
endif

ifndef verbose
  SILENT = @
endif

ifeq ($(config),debug_x32)
  misstortion_lv2_config = debug_x32
endif
ifeq ($(config),debug_x64)
  misstortion_lv2_config = debug_x64
endif
ifeq ($(config),release_x32)
  misstortion_lv2_config = release_x32
endif
ifeq ($(config),release_x64)
  misstortion_lv2_config = release_x64
endif

PROJECTS := misstortion_lv2

.PHONY: all clean help $(PROJECTS) 

all: $(PROJECTS)

misstortion_lv2:
ifneq (,$(misstortion_lv2_config))
	@echo "==== Building misstortion_lv2 ($(misstortion_lv2_config)) ===="
	@${MAKE} --no-print-directory -C . -f misstortion_lv2.make config=$(misstortion_lv2_config)
endif

clean:
	@${MAKE} --no-print-directory -C . -f misstortion_lv2.make clean

